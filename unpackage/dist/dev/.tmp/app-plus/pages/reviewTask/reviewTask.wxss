
.class{
	width: 100%;
	height: auto;
	overflow: scroll;
}
::-webkit-scrollbar{
	height: 6rpx;
	width: 2rpx;
}
#title{ 
	height: 70rpx;
	width: 100%;
	line-height: 70rpx;
	font-weight: bold;
	font-size: 30rpx;
	background-color: #9BC4CE;
	margin-left: 10rpx;
}
.reviewList{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	width: 100%;
}
.eachOneTask{
	margin-top: 10rpx;
	width: 100%;
}
.Taskdetail text{
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
}
.Taskdetail{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	font-size: 30rpx;
}

