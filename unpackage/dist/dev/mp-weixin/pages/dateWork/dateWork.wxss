
.all{
	width: 100%;
	height: auto;
    overflow-y: scroll;
}
.dateWork{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	margin-top: 10rpx;
}
.dateWork view{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	margin-top: 10rpx;
}
.dateWork view text{
	color: #F0F8FF;
	margin-left: 8rpx;
	font-size: 30rpx;
	margin-top: 20rpx;
}
#title{
	height: 60rpx;
	background-color:#5087BF;
	line-height: 60rpx;
	font-weight: bold;
	font-size: 30rpx;
	color: #3F536E!important;
	margin-left: 0rpx!important;
}
.dateWorkDetail1,.dateWorkDetail2{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal!important;
	-webkit-box-direction: normal!important;
	-webkit-flex-direction: row!important;
	    -ms-flex-direction: row!important;
	        flex-direction: row!important;
	-webkit-box-align: center!important;
	-webkit-align-items: center!important;
	    -ms-flex-align: center!important;
	        align-items: center!important;
	margin-top: 10rpx;
}
.dateWorkDetail1 text{
    height: 60rpx;
	line-height: 60rpx;
    margin-top: 0rpx!important;
	width: 120rpx;
	margin-left: 10rpx;
	color: #F1F1F1;
}
.dateWorkDetail1 input{
	height: 60rpx;
	line-height: 60rpx;
	color: #F0F8FF;
	font-size: 30rpx;
}
.dateWorkDetail2 text{
	height: 250rpx;
	line-height: 250rpx;
	width: 100rpx;
	margin-left: 10rpx;
	margin-top: 0rpx!important;
	color: #F1F1F1;
}
.dateWorkDetail2 textarea{
	height: 250rpx;
	border-radius: 2%;
	border: 1px solid #C0C0C0;
	font-size: 30rpx;
	font-size: 30rpx;
	overflow: scroll;
	color: #F8F1F3;
}
.detailStatus{
	position: relative;
	width: 100%;
	height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.detailStatus input{
	height: 70rpx;
	line-height: 70rpx;
	border-radius: 5%;
	line-height: 70rpx;
	font-size: 30rpx;
	text-align: center;
	color: #F1F1F1;
	margin-top: 10rpx;
}
#arrow{
	position: absolute;
	right: 50rpx;
	top: -10rpx;
	font-weight: bold;
	height: 70rpx;
	line-height: 70rpx;
}
#slider{
	margin-left: 10rpx;
}
.submit{
	height: 70rpx;
	line-height: 70rpx;
	width: 70%;
	background-color: #6AA2D4;
    font-size: 35rpx;
	font-weight: 500;
}

