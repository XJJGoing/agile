<scroll-view class="all" scroll-y="true"><view class="search_project"><button class="ibutton" style="color:#fbf3f5;border:1px solid #fbf3f5;" plain="true" data-event-opts="{{[['tap',[['bindChange']]]]}}" bindtap="__e">查看项目</button><block wx:if="{{isLookApply}}"><block><view class="applyList"><view class="appListProjectName"><text>项目编号:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="请输入项目的编号..." data-event-opts="{{[['input',[['inputProjectName',['$event']]]]]}}" value="{{projectName}}" bindinput="__e"/></view><view class="appListView1"><text>申请人:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="请填入真实的姓名..." data-event-opts="{{[['input',[['inputName',['$event']]]]]}}" value="{{trueName}}" bindinput="__e"/></view><view class="appListView2"><text>理由:</text><textarea style="{{'width:'+(width+'px')+';'}}" placeholder="填入申请查看项目的理由" data-event-opts="{{[['input',[['inputWhy',['$event']]]]]}}" value="{{why}}" bindinput="__e"></textarea></view><button class="submitApply" style="color:#fbf3f5;border:1px solid #fbf3f5;" plain="true" data-event-opts="{{[['tap',[['submitApply',['$event']]]]]}}" bindtap="__e">提交申请</button></view></block></block><button class="ibutton" style="color:#fbf3f5;border:1px solid #fbf3f5;" plain="true" data-event-opts="{{[['tap',[['applyProject']]]]}}" bindtap="__e">申请项目</button><block wx:if="{{isApplyProject}}"><block><view class="applyList"><view class="appListProjectName"><text>项目编号:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="填入项目编号(即名称15字以内)" data-event-opts="{{[['input',[['inputApplyProjectName',['$event']]]]]}}" value="{{applyProjectName}}" bindinput="__e"/></view><view class="appListView1"><text>申请人:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="请填入真实的姓名..." data-event-opts="{{[['input',[['inputApplyTrueName',['$event']]]]]}}" value="{{applyTrueName}}" bindinput="__e"/></view><view class="appListView2"><text>理由:</text><textarea style="{{'width:'+(width+'px')+';'}}" placeholder="填入申请项目的理由" data-event-opts="{{[['input',[['inputApplyWhy',['$event']]]]]}}" value="{{applyWhy}}" bindinput="__e"></textarea></view><button class="submitApply" style="color:#fbf3f5;border:1px solid #fbf3f5;" plain="true" data-event-opts="{{[['tap',[['submitApplyProject',['$event']]]]]}}" bindtap="__e">提交申请</button></view></block></block></view><view class="ableProjects"><view class="titleInfo"><uni-tag vue-id="1" text="可看的项目:" type="primary" bind:__l="__l"></uni-tag></view><block wx:if="{{ableLookProject}}"><block><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="myHadProject"><button id="{{item.g0}}" data-event-opts="{{[['tap',[['enterProject',['$event']]]]]}}" bindtap="__e">{{item.$orig.projectName}}</button></view></block></block></block><view class="titleInfo"><uni-tag vue-id="2" text="负责的项目:" type="warning" bind:__l="__l"></uni-tag></view><block wx:if="{{chargeProject}}"><block><block wx:for="{{$root.l1}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="myHadProject"><button id="{{item.g1}}" data-event-opts="{{[['tap',[['enterProject',['$event']]]]]}}" bindtap="__e">{{item.$orig.projectName}}</button></view></block></block></block><view class="titleInfo"><uni-tag vue-id="3" text="参与的项目" type="error" bind:__l="__l"></uni-tag></view><block wx:if="{{joinProject}}"><block><block wx:for="{{$root.l2}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="myHadProject"><button id="{{item.g2}}" data-event-opts="{{[['tap',[['enterProject',['$event']]]]]}}" bindtap="__e">{{item.$orig.projectName}}</button></view></block></block></block></view></scroll-view>