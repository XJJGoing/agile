<scroll-view class="project" scroll-y="true"><view class="chooseItem"><picker value="{{index}}" range="{{myProject}}" data-event-opts="{{[['change',[['bindPickerMyProject',['$event']]]]]}}" bindchange="__e"><text>切换首页冲刺</text></picker><picker value="{{index}}" range="{{myLookProject}}" data-event-opts="{{[['change',[['bindPickerLookProject',['$event']]]]]}}" bindchange="__e"><text>切换可看项目</text></picker></view><view class="titleView"><text class="title">项目信息:</text></view><view class="projectInfo"><view><text>项目编号:</text><input style="{{'width:'+(width+'px')+';'}}" disabled="true" value="{{nowProject.projectName}}"/></view><view><text>项目目标:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="填入项目的目标" disabled="{{isChooseInput}}" data-event-opts="{{[['input',[['ftarget',['$event']]]]]}}" value="{{nowProject.projectTarget}}" bindinput="__e"/></view><view><text>交付时间:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="填入项目交付的时间" disabled="{{isChooseInput}}" data-event-opts="{{[['input',[['ffinishTime',['$event']]]]]}}" value="{{nowProject.projectFinishTime}}" bindinput="__e"/></view><view><text>项目成果:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="填入项目的成果" disabled="{{isChooseInput}}" data-event-opts="{{[['input',[['fresult',['$event']]]]]}}" value="{{nowProject.projectResult}}" bindinput="__e"/></view><view><text>项目管理:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="填入项目的管理" disabled="{{isChooseInput}}" data-event-opts="{{[['input',[['fmanagement',['$event']]]]]}}" value="{{nowProject.projectManagement}}" bindinput="__e"/></view><view><text>冲刺个数:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="填入项目冲刺的个数" disabled="{{isChooseInput}}" data-event-opts="{{[['input',[['fprintNum',['$event']]]]]}}" value="{{nowProject.projectSprintNum}}" bindinput="__e"/></view><view><text>项目成员:</text><input style="{{'width:'+(width+'px')+';'}}" placeholder="填入项目的成员(输入数字)" disabled="{{isChooseInput}}" data-event-opts="{{[['input',[['fpeople',['$event']]]]]}}" value="{{nowProject.projectPeople}}" bindinput="__e"/></view></view><block wx:if="{{roleId===1}}"><block><view class="footer"><block wx:if="{{roleId===1&&isDisplay}}"><block><q-s-waves-button class="QSbutton" vue-id="1" btnType="default" plain="true" wavesColor="rgba(211,220,114,0.93)" btnStyle="{font-size:35upx;color: #E9EFED;}" data-event-opts="{{[['^click',[['saveProject']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><text>提交</text></q-s-waves-button></block></block><q-s-waves-button class="QSbutton" vue-id="2" btnType="default" plain="true" wavesColor="rgba(114,220,158,0.93)" btnStyle="{font-size:35upx;color:#E9EFED;}" data-event-opts="{{[['^click',[['addSprint']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><text>添加冲刺</text></q-s-waves-button></view></block></block><q-s-waves-button class="QSbutton" vue-id="3" btnType="default" plain="true" wavesColor="#e4712b" btnStyle="{font-size:35upx;color:#E9EFED;width:400upx;}" data-event-opts="{{[['^click',[['returnApply']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><text>申请查看其他项目</text></q-s-waves-button></scroll-view>