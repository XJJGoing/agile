
.project{
	width: 100%;
	height: auto;
	overflow: scroll;
}
::-webkit-scrollbar{
	height: 6rpx;
	width: 2rpx;
}
.chooseItem{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient:horizontal;
	-webkit-box-direction:normal;
	-webkit-flex-direction:row;
	    -ms-flex-direction:row;
	        flex-direction:row;
	-webkit-justify-content: space-around;
	    -ms-flex-pack: distribute;
	        justify-content: space-around;
	margin-bottom: 30rpx;
	margin-top: 30rpx;
}
.chooseItem picker{
	height: 55rpx;
	width: 200rpx;
	border-radius: 3%;
	text-align: center;
	background-color:rgb(5,224,252);
	font-size: 13px;
	line-height: 55rpx;
    color: #F0F8FF;
}
.chooseItem picker text{
	height: 55rpx;
	width: 200rpx;
	line-height: 55rpx;
    font-size:12px;
	text-align: center;
	color: #F0F8FF;
	font-weight: 600;
}
.titleView{
  width: 100%;
  height: 50rpx;
  text-align: center;
  background-color:rgb(5,224,252);
  color: #F0F8FF;
  margin-left: 0rpx;
  border-radius: 2%;
}
.title{
	width: 100%;
	height: 50rpx;
	line-height: 50rpx;
	font-size: 30rpx;
	font-weight: 600;
	font-family: webfont;
	color: #F0F8FF;
}
.projectInfo{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
}
.projectInfo view{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	height: 70rpx;
	width: 100%;
	padding: 20rpx;
	margin-top: 3rpx;
}
.projectInfo view text{
  font-size:30rpx;
  width: 140rpx;
  font-weight: 500;
  color:#FCFFFE;
}
.projectInfo view input{
	font-size:28rpx;
	height: 60rpx;
	margin-left: 5rpx;
	color: #FEFAFB;
	line-height: 60rpx;
}
.footer{ 
	display: -webkit-box; 
	display: -webkit-flex; 
	display: -ms-flexbox; 
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	width: 100%;
}
.QSbutton{
	margin-top: 5rpx;
	width: 50%;
	height: 60rpx;
	line-height: 60rpx;
	margin-top: 10rpx;
	margin-bottom: 30rpx;
}
.QSbutton text{
	color:#E9EFED;
}
.return-apply-btn{
	color: #F0F8FF!important;
	width: 60%;
	border: 1px solid #E9EFED;
	font-size: 30rpx!important; 
	height: 90rpx;
	margin-top: 20rpx;
	line-height: 90rpx;
}
/*字体设置*/
@font-face {
  font-family: 'webfont';
  font-display: swap;
  src: url('//at.alicdn.com/t/webfont_i271qcyeekd.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_i271qcyeekd.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/webfont_i271qcyeekd.woff2') format('woff2'),
  url('//at.alicdn.com/t/webfont_i271qcyeekd.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/webfont_i271qcyeekd.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/webfont_i271qcyeekd.svg#杨任东竹石体-Bold') format('svg'); /* iOS 4.1- */
}

