<view class="all"><text class="title">更新预估工时</text><form report-submit="true" data-event-opts="{{[['submit',[['submit',['$event']]]]]}}" bindsubmit="__e"><view><text>任务序号:</text><input style="{{'width:'+(width+'px')+';'}}" disabled="true" value="{{task.taskOrder}}"/></view><view><text>任务内容:</text><input style="{{'width:'+(width+'px')+';'}}" disabled="true" value="{{task.taskName}}"/></view><block wx:if="{{enterId===1}}"><block><view><text>预估工时:</text><input style="{{'width:'+(width+'px')+';'}}" disabled="{{canChangeTaskPredictTime}}" data-event-opts="{{[['input',[['inputPredictTime',['$event']]]]]}}" value="{{taskPredictTime}}" bindinput="__e"/></view></block></block><block wx:if="{{enterId===2}}"><block><view><text>优先级:</text><picker style="{{'width:'+(width+'px')+';'}}" range="{{priorityArry}}" value="{{index}}" data-event-opts="{{[['change',[['pickerChangePriority',['$event']]]]]}}" bindchange="__e">{{taskPriority}}</picker></view></block></block><button class="submintBtn" form-type="submit">提交</button></form></view>